      <Property Name="BackColor">LightYellow</Property>
      <Property Name="Font">Microsoft Sans Serif, 9.360001pt, style=Bold</Property>
      <Property Name="Text">2 Connecting the VT7001 module to other VT modules</Property>
    </Object>
    <Object Type="Vector.CANalyzer.Panels.Design.StaticTextControl, Vector.CANalyzer.Panels.CommonControls, Version=7.6.42.0, Culture=neutral, PublicKeyToken=null" Name="StaticTextControl3" Children="Controls" ControlName="Static Text">
      <Property Name="Name">StaticTextControl3</Property>
      <Property Name="Size">480, 39</Property>
      <Property Name="Location">33, 64</Property>
      <Property Name="BackColor">LightYellow</Property>
      <Property Name="Text">The VT7001 module can be used to control up to two power supply inputs of an ECU (e.g. terminals 15 and 30). As an alternative, two ECUs may be supplied separately, e.g. in testing differences in voltage supplies.</Property>
    </Object>
