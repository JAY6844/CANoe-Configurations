<ttroot version="15" xmlns="http://www.vector-informatik.de/ITE/TestTable/1.0">
  <tt>
    <variables />
    <tc>
      <title>Test Startup</title>
      <comment>
        <title>Initialze the XIL API</title>
      </comment>
      <netfunction>
        <title />
        <name>Init</name>
        <class>XILWrapper</class>
        <assembly />
        <param>
          <type>String</type>
          <value>
            <const>../XIL API Configuration</const>
          </value>
        </param>
        <param>
          <type>String</type>
          <value>
            <const>Example Vendor</const>
          </value>
        </param>
        <param>
          <type>String</type>
          <value>
            <const>Example Product</const>
          </value>
        </param>
        <param>
          <type>String</type>
          <value>
            <const>1.0.0.0</const>
          </value>
        </param>
      </netfunction>
    </tc>
    <tc>
      <title>Test Read/Write</title>
      <comment>
        <title>Set the variable to 5</title>
      </comment>
      <netfunction>
        <title />
        <name>WriteVariable</name>
        <class>XILWrapper</class>
        <assembly />
        <param>
          <type>String</type>
          <value>
            <const>TestVariable</const>
          </value>
        </param>
        <param>
          <type>Double</type>
          <value>
            <const>5.0</const>
          </value>
        </param>
      </netfunction>
      <comment>
        <title>Read and check the variable's value</title>
      </comment>
      <netfunction>
        <title />
        <name>ReadVariable</name>
        <class>XILWrapper</class>
        <assembly />
        <param>
          <type>String</type>
          <value>
            <const>TestVariable</const>
          </value>
        </param>
        <returnvalue>
          <type>Double</type>
          <value>
            <dbobject>SysVar_BEGIN_OF_OBJECT|1|ReadValue|Test|-1|0|END_OF_OBJECT_SysVar|</dbobject>
          </value>
        </returnvalue>
      </netfunction>
      <statecheck>
        <title />
        <expected>
          <compare>
            <dbobject>SysVar_BEGIN_OF_OBJECT|1|ReadValue|Test|-1|0|END_OF_OBJECT_SysVar|</dbobject>
            <eq>
              <const>5</const>
            </eq>
          </compare>
        </expected>
      </statecheck>
      <comment>
        <title>Set the variable to 42</title>
      </comment>
      <netfunction>
        <title />
        <name>WriteVariable</name>
        <class>XILWrapper</class>
        <assembly />
        <param>
          <type>String</type>
          <value>
            <const>TestVariable</const>
          </value>
        </param>
        <param>
          <type>Double</type>
          <value>
            <const>42.0</const>
          </value>
        </param>
      </netfunction>
      <comment>
        <title>Read and check the variable's value</title>
      </comment>
      <netfunction>
        <title />
        <name>ReadVariable</name>
        <class>XILWrapper</class>
        <assembly />
        <param>
          <type>String</type>
          <value>
            <const>TestVariable</const>
          </value>
        </param>
        <returnvalue>
          <type>Double</type>
          <value>
            <dbobject>SysVar_BEGIN_OF_OBJECT|1|ReadValue|Test|-1|0|END_OF_OBJECT_SysVar|</dbobject>
          </value>
        </returnvalue>
      </netfunction>
      <statecheck>
        <title />
        <expected>
          <compare>
            <dbobject>SysVar_BEGIN_OF_OBJECT|1|ReadValue|Test|-1|0|END_OF_OBJECT_SysVar|</dbobject>
            <eq>
              <const>42</const>
            </eq>
          </compare>
        </expected>
      </statecheck>
    </tc>
